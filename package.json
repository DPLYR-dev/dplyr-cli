{
  "name": "dplyr-cli",
  "description": "A CLI tool to manage DPLYR",
  "version": "0.0.2",
  "author": "Ahmed Gamal @ahmedmgh67",
  "bin": {
    "dplyr": "./bin/run"
  },
  "bugs": "https://github.com/dplyr-dev/dplyr-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-update": "^1.3.10",
    "@types/fs-extra": "^8.1.0",
    "axios": "^0.20.0",
    "base64-url": "^2.3.3",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "simple-git": "^2.20.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/dplyr-dev/dplyr-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dplyr",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-update"
    ],
    "topics": {
      "deployments": {
        "description": "Create and preview Deployments from here"
      },
      "machines": {
        "description": "Create, delete, list and preview machines on DPLYR"
      }
    }
  },
  "repository": "https://github.com/dplyr-dev/dplyr-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
